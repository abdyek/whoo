{
    "name":"abdyek/whoo",
    "description": "database agnostic authentication library",
    "keywords": [
        "authentication",
        "jwt"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yunus Emre Bulut",
            "email": "yunusemrebulut123@gmail.com"
        }
    ],
    "require": {
        "firebase/php-jwt": "^5.4",
        "propel/propel": "2.0.0-beta1",
        "php-di/php-di": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.99"
    },
    "autoload": {
        "psr-4": {
            "Abdyek\\Whoo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pseudo\\": "tests/unit/pseudo"
        },
        "classmap": [
            "tests/unit/traits",
            "propel/model"
        ]
    },
    "bin": [
        "bin/whoo"
    ]
}
